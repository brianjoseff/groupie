%header
	=link_to logo, root_path
	%nav
		%ul		
			%li= link_to "about", about_path
			-if signed_in?
				%li= link_to "post",  new_user_post_path(current_user)
				%li= link_to "home", user_path(current_user)
				%li= link_to "sign out", signout_path
			-else
				%li= link_to "sign in", signin_path