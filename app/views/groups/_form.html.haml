-form_for(:group, :url => groups_path, :html => { :multipart => true}) do |f|
	%dl
		%dt=f.label :photo
		%dd=f.file_field :photo
		%dt=f.label :name
		%dd=f.text_field :name
		%dt=f.label :private, "private?"
		%dd=f.check_box :private		
		%dt=f.label :description
		%dd=f.text_area :description
		=f.submit "Create"